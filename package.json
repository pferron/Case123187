    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "9.4.3",
    "@nestjs/event-emitter": "1.4.2",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-fastify": "9.4.3",
    "@nestjs/schedule": "2.2.3",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/typeorm": "9.0.1",
    "@ntegral/nestjs-sentry": "4.0.0",
    "axios": "1.5.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "date-fns": "2.30.0",
    "dd-trace": "3.37.0",
    "fastify": "4.23.2",
    "ioredis": "5.3.2",
    "joi": "17.11.0",
    "kafkajs": "2.2.4",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nestjs-pino": "3.5.0",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.3",
    "pino": "8.15.6",
    "pino-pretty": "10.2.2",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17",
    "typeorm-naming-strategies": "4.1.0",
    "typescript": "5.2.2",
    "uuid": "9.0.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@types/amqplib": "0.10.5",
    "@types/cache-manager": "4.0.3",
    "@types/cron": "2.0.1",
    "@types/ioredis": "4.28.10",
    "@types/jest": "29.5.5",
    "@types/joi": "17.2.3",
    "@types/lodash": "4.14.199",
    "@types/ms": "0.7.32",
    "@types/node": "18.18.3",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.10",
    "@types/pg": "8.10.3",
    "@types/uuid": "9.0.4",
    "@types/validator": "13.11.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "better-npm-audit": "3.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jest-watch-typeahead": "2.2.2",
    "jsonwebtoken": "9.0.2",
    "lint-staged": "14.0.1",
    "organize-imports-cli": "0.10.0",
    "prettier": "3.0.3",
    "rimraf": "3.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,ts}": [
      "organize-imports-cli",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "scripts": {
    "audit": "better-npm-audit audit --production --level=high",
    "build": "nest build",
    "build:watch": "tsc -w",
    "db:migrate": "npm run typeorm -- migration:run",
    "db:revert": "npm run typeorm -- migration:revert",
    "db:migrate:warehouse": "npm run typeorm:warehouse -- migration:run",
    "db:migrate:all": "npm run db:migrate:warehouse && npm run db:migrate",
    "depcheck": "npx depcheck",
    "jwt:generate": "ts-node bin/create-valid-jwt.ts",
    "prettier": "prettier --write --ignore-path .gitignore .",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "prestart": "node dist/src/setup-db.js && npm run db:migrate",
    "start": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./src/e2e/jest-e2e.json --runInBand --detectOpenHandles",
    "test:e2e:cov": "npm run test:e2e -- --coverage",
    "test:all:cov": "jest --config ./src/e2e/jest-all.json --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "typeorm": "typeorm -d dist/src/data-source/data-source",
    "typeorm:generate": "npm run typeorm migration:generate -- --pretty",
    "typeorm:warehouse": "typeorm -d dist/src/data-source/data-source-warehouse",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prepare": "husky install"
  }
}
